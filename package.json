{
  "name": "classroom-emailer",
  "sideEffects": false,
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --env=prod && zip -j function.zip .env build/index.js",
    "upload": "aws lambda --profile gokibitz update-function-code --function-name ginkgo-emailer --zip-file fileb://function.zip",
