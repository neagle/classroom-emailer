    "dotenv": "^8.1.0",
    "googleapis": "^42.0.0",
    "nodemailer": "^6.4.16",
    "nodemailer-smtp-transport": "^2.7.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "jest": "^24.9.0",
    "lambda-local": "^1.6.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.7"
  },
  "babel": {
